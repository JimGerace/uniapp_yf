{
  "pages": [
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "navigationBarBackgroundColor": "#ff0000",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/find/find",
      "style": {
        "navigationBarTitleText": "发现"
      }
    }
  ],
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#ff0000",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/my/my",
        "iconPath": "./static/images/tabBar/icon_my_default.png",
        "selectedIconPath": "./static/images/tabBar/icon_my_active.png",
        "text": "我的"
      },
      {
        "pagePath": "pages/find/find",
        "iconPath": "./static/images/tabBar/icon_find_default.png",
        "selectedIconPath": "./static/images/tabBar/icon_find_active.png",
        "text": "发现"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#ff0000",
    "backgroundColor": "#F8F8F8",
    "h5": {
      "titleNView": false
    }
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "subPackages": [
    {
      "root": "subPackages/cloud",
      "pages": [
        {
          "path": "cloud",
          "style": {
            "navigationBarTitleText": "云盘"
          }
        }
      ]
    },
    {
      "root": "subPackages/recently",
      "pages": [
        {
          "path": "recently",
          "style": {
            "navigationBarTitleText": "最近播放"
          }
        }
      ]
    }
  ]
}
